<meta charset="utf-8">

<script>
	
	function pularLinha() {
		document.write("<br><br>");
	}

	function mostrar(frase) {
		document.write(frase);
		pularLinha();
	}

	var numero = Math.round(Math.random()*10);
	var numeroPensado = parseInt(prompt("Qual número? "));

	if(numeroPensado == numero){
		mostrar("Você acertou!");
		mostrar("O número pensado é o: " + numero);
	}

	else{
		mostrar("você errou!");
		mostrar("O número pensado é o: " + numero);
	}


</script>








