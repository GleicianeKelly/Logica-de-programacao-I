<meta charset="utf-8">

<script>
	
	function pularLinha() {
		document.write("<br><br>");
	}

	function mostra(imc) {
		document.write(imc);
		pularLinha();
	}



	var altura = 1.65;
	var peso = 60;
	var imc = peso/(altura * altura);
	

	mostra("O imc de Gleiciane é: " + Math.round(imc));


